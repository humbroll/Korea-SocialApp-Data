<table>
  <tr>
    <th>Name</th>
    <th>Appid</th>
    <th>Author</th>
    <th>Description</th>
    <th>Category</th>
    <th>Birthday</th>
    <th>Platform</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @apps.each do |app| %>
  <tr>
    <td><%= app.name %></td>
    <td><%= app.appId %></td>
    <td><%= app.author %></td>
    <td><%= app.description %></td>
    <td><%= app.category %></td>
    <td><%= app.birthday %></td>
    <td><%= app.platform %></td>
    <td><%= link_to 'Show', app %></td>
    <td><%= link_to 'Edit', edit_app_path(app) %></td>
    <td><%= link_to 'Destroy', app, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

