<!-- Scaffolding generated by Casein v.3.1.7 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>Rank</th>
		<th>Ordertype</th>
		<th>Rating</th>
		<th>Downloadcount</th>
		<th>App</th>
		<th>&nbsp;</th>
	</tr>
	
	<% ranks.each_with_index do |rank, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
			<td><%= casein_table_cell_link rank.rank, casein_rank_path(rank) %></td>
			<td><%= casein_table_cell_link rank.orderType, casein_rank_path(rank) %></td>
			<td><%= casein_table_cell_link rank.rating, casein_rank_path(rank) %></td>
			<td><%= casein_table_cell_link rank.downloadCount, casein_rank_path(rank) %></td>
			<td><%= casein_table_cell_link rank.app_id, casein_rank_path(rank) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_rank_path(rank), :method => :delete, :confirm => "Are you sure you want to delete this rank?") %>
			</td>
		</tr>
	<% end %>
</table>